import java.util.LinkedHashMap;
import java.util.Map;
import java.util.Scanner;

public class LegendaryFarming {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);

        String[] input = sc.nextLine().split(" ");
        Map<Integer, String> shadowmourne = new LinkedHashMap<>();
        Map<Integer, String> valanyr = new LinkedHashMap<>();
        Map<Integer, String> dragonwrath = new LinkedHashMap<>();
        Map<Integer, String> junk = new LinkedHashMap<>();
        
        for (int i = 0; i < input.length; i++) {
            int quantity = Integer.parseInt(input[0]);
            String material = input[1].toLowerCase();
        }
    }
}
